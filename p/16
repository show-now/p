import matplotlib.pyplot as plt

students = ['Arjuna', 'Bheema', 'Chirag', 'Dhruv', 'Eesho']
marks = [85, 90, 70, 95, 80]

plt.figure(figsize=(6, 4))
plt.plot(students, marks, color='blue')
plt.title("Student Marks - Line Graph")
plt.xlabel("Students")
plt.ylabel("Marks")
plt.grid(True)
plt.show()

plt.figure(figsize=(6, 4))
plt.bar(students, marks, color='orange')
plt.title("Student Marks - Bar Chart")
plt.xlabel("Students")
plt.ylabel("Marks")
plt.show()
