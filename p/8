import pandas as pd

nums = pd.Series([10, 20, 30, 40, 50, 60])

print("Series:")
print(nums)

print("--- Indexing ---")
print("Index 0:", nums[0])
print("Index 5:", nums[5])

print("--- Slicing ---")
print("Slicing from 0 to 3:\n", nums[0:3])
print("Slicing from 3 to 5:\n", nums[3:5])

print("--- Querying ---")
print("Numbers greater than 25:\n", nums[nums > 25])
print("Numbers smaller than 25:\n", nums[nums < 25])
